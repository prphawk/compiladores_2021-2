int fib(int a) {
  if (a == 0) {
    return 0;
  };
  if (a == 1) {
    return 1;
  };
  return fib(a-1)+fib(a-2);
}

int main() {
  int fib6;
  int constante <= 1000;
  int fator <= 3;
  int fator_constante;
    
  fator_constante = fator * constante;

  fib6 = fib(1) + fib(2);

  return fator_constante * fib6;
}
